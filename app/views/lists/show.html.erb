<h1><%= @list.name %></h1>

<% @list.bookmarks.each do |bookmark| %>
<%= Movie.find(bookmark.movie_id) %>
<%= link_to "Remove this movie", bookmark_path(bookmark),
    data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}%>
<% end %>

<%= link_to "Add bookmark",  new_list_bookmark_path(@list.id) %>
